				<select id="i_SearchOpt_new" name="cx">
				
				<option value="artists" >discographies</option>
				<option value="forums" >forums</option>
				</select> 
				<input type="button" name="sa" id="i_SearchBtn" value="submit" onclick="javascript:submitSearch();" />
				</li>
				</ul>
				
